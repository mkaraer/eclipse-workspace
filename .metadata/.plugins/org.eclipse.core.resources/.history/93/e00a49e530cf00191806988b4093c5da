package day8_conditions;

public class numbersCompare {
	public static void main(String[] args) {
		int num1,num2;
		num1=100;
		num2=200;
		if (num1==num2) {
			System.out.println("numbers are equal");{
				} 
			if (num1>num2) {
					System.out.println("num1 is greater");
				}
			if (num2>num1) {
					System.out.println("number2 is grater");
				}
			}
		
		
		
		
		
		
		
		
		
		
	}
}
