package day55_warmUpPolymorphizm;

public class NewYear extends Holiday {
	int dayOffCount;
	
	@Override
	public void sendHolidayMessage() {
		System.out.println("Happy newyear "+dayOffCount);
		super.sendHolidayMessage();
	}
	public void exchangeGifts() {
		System.out.println("Exchange gift ");
	}
	

}
