package day26_;

public class arrayWaruuIncrementValues {
	public static void main (String[] args) {
		
		int[] nums= {1,2,3,4,5,6,7,8,9};
		
		int odd=nums[0];
		int even=nums[0];
       for(int n:nums) {
    	   System.out.print(n+" ");
       }
       System.out.println(" ");
       for(int n:nums) { 
       if(n%2==0) {
    		  even+=n+2;
    		  System.out.println(even);
    		  
    	   
    	   }else {
    		   odd+=n+3;
    		   
    	   }
    	   
    	   
       }
       System.out.println("odd "+odd);
       
       
       System.out.println("even Number "+even);
		
       
		
		
		
	}
}
