package day_11Switch;

public class grace {
	public static void main(String[] args) {
		
		char grade='A';
		
		switch (grade) {
		case 'A':
			System.out.println("Excellent");
			break;
		case 'B':
			System.out.println("Good");
			break;
		case 'C':
			System.out.println("accaptable");
			break;
		case 'D':
			System.out.println("Bad");
			
			default:
				System.out.println("invalid");
				break;
			
		
		
		
		
		
		
		
		
		}
		
		
		
		
		
		
	}
}
