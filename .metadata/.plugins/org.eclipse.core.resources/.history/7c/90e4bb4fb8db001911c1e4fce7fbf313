package day26_;

public class arrayWaruuIncrementValues {
	public static void main (String[] args) {
		
		int[] nums= {1,2,3,4,5,6,7,8,9};
		
		
		int even=nums[0];
       for(int n:nums) {
    	   if(n%2==0) {
    		   even+=n+2;
    	   }
    	   
       }
       System.out.print("even Number "+even);
		
		
		
		
		
		
		
	}
}
