package day42_costructorThis;

public class studentTester {
public static void main(String[] args) {
	

	student s=new student();
	s.setName("ahmed");
	s.setLast("husnittin");
	s.setId(12);
	
	System.out.println(s.getName());
	
	student[] st=new student[3];
	st[0]=s;
	for(student r:st) {
		System.out.println(r.getName());
	}
	
	
	
	
	
	
	
}	
}
