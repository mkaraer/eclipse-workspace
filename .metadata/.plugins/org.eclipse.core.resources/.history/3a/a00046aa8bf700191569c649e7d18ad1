package day60_Collection;

import java.util.*;

public class Practice4 {
public static void main(String[] args) {
	
	String[] arr= {"Apple","Lemon","Mango","Ayse","Hilal"};
	List<String> lst=new ArrayList<>();
	
	for(String each:arr) {
		lst.add(each);
	}
System.out.println(lst);
	
	
	
	
	
	
	
	
	
	
	
	
	
}
}
