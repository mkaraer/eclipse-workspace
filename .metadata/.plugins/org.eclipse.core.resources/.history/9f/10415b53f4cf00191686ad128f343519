package day9_conditions;

public class rangeCheck {
   public static void main(String[] args) {
	   
	   int students=25;
	   
	   if(students>=5&&students<30) {
		   System.out.println("Lets do montoring" );
	   }else if(students>=30&& students<=60) {
		   System.out.println("lets have a lecture");
	   }else if (students>60) {
		   int online= students-60;
		   System.out.println(online+"students need to join online");
	   }
	   
	 
	 
	 
	 
 }
}
