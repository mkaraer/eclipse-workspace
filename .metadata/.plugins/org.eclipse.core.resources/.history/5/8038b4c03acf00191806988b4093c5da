package day8_conditions;

public class grades {
	public static void main(String[] args) {

//		A,B,C
//		USE Cahar variables
//		if grade is A excellent B- dood c- acceptable d fair
		char A= excellent;
		char B= good;
		char C= acceptable;
		char D= fair;
			
		if (A== excellent) {
			System.out.println("welldone.");
			
		}else if(B== good) {
			System.out.println("you are good");
		}else if (C== acceptable) {
			System.out.println("It is Ok");
		}else if(D== fair) {
			System.out.println("try your best");
		}
		
		
		
		
	}
}
